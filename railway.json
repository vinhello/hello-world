{
    "build": {
      "frontend": "cd frontend && npm install && npm run build",
      "backend": "cd backend && pip install -r requirements.txt"
    },
    "start": {
      "frontend": "cd frontend && npx serve -s build -l 3000",
      "backend": "cd backend && gunicorn -w 4 -b 0.0.0.0:5000 app:app"
    }
  }
  